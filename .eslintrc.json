{
  "parser": "babel-eslint",
  "extends": [],
  "env": {
    "es6": true
  },
  "plugins": [],
  "globals": {
    "__DEV__": false,
    "fetch": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "15.4.0",
      "flowVersion": "0.83"
    }
  },
  "rules": {
    "no-console": 0,
    "react-native/no-inline-styles": 0,
    "react/prop-types": 0,
    "react-native/no-color-literals": 0,
    "react/display-name": 0,
    "no-extra-boolean-cast": 0,
    "react/no-string-refs": 0,
    "no-useless-escape": 0,
    "react-native/split-platform-components": 0,
    "max-depth": 0,
    "max-statements": 0,
    "complexity": 0,
    "max-params": 0,
    "max-nested-callbacks": 0,
    "radix": 0
  }
}
